<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >

    
          <TextView 
              	android:id="@+id/baojia_list_item_txt1"
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:layout_weight="1"
        		android:text="检测区域"
        		android:textSize="20dip"
        		android:gravity="center_horizontal"        		
                />
            <TextView 
                android:id="@+id/baojia_list_item_txt2"
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:layout_weight="1"
        		android:text="检测门店"
        		android:textSize="20dip"
        		android:gravity="center_horizontal"
                />
            <TextView 
                android:id="@+id/baojia_list_item_txt3"
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:layout_weight="1"
        		android:text="检测人"
        		android:textSize="20dip"
        		android:gravity="center_horizontal"
                />
            <TextView 
                android:id="@+id/baojia_list_item_txt4"
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:layout_weight="1"
        		android:text="检测完成时间"
        		android:textSize="20dip"
        		android:gravity="center_horizontal"
                />
            <TextView 
                android:id="@+id/baojia_list_item_txt5"
                android:layout_width="fill_parent"
        		android:layout_height="wrap_content"
        		android:layout_weight="1"
        		android:text="评估价"
        		android:textSize="20dip"
        		android:gravity="center_horizontal"
                />
            
</LinearLayout>
    